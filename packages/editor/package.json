{
  "name": "@react-av/editor",
  "private": false,
  "version": "0.0.1",
  "description": "Editor Timeline Components built on React AV.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build": "pnpm build:cjs && pnpm build:esm",
    "dev": "tsc --watch"
  },
  "keywords": [
    "video",
    "editor",
    "timeline",
    "react",
    "react-av",
    "subtitles",
    "captions",
    "audio",
    "media",
    "player"
  ],
  "author": "Daniel Wykerd <daniel@wykerd.dev>",
  "license": "MIT",
  "repository": {
    "directory": "packages/editor",
    "type": "git",
    "url": "https://github.com/Wykerd/react-av.git"
  },
  "devDependencies": {
    "@react-av/core": "workspace:^",
    "@react-av/vtt": "workspace:^",
    "@react-av/vtt-core": "workspace:^",
    "@react-av/controls": "workspace:^",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@react-av/core": "workspace:^",
    "@react-av/vtt": "workspace:^",
    "@react-av/vtt-core": "workspace:^",
    "@react-av/controls": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "use-resize-observer": "^9.1.0",
    "@phosphor-icons/react": "^2.1.7"
  }
}
